@model IEnumerable<HotelBookingWebsite.Models.InventoryModel>

<table class="table">
    <tr>
        <th>
            Room Type
        </th>
        <th>
            Total Amount 
        </th>
        <th>
            Number of Rooms Occupied
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @* TODO make it mutable *@
            @Html.DisplayFor(modelItem => item.type)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.inventory)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.occupancy)
        </td>
    </tr>
}

</table>
